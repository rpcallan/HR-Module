        <name>Mobile Phone Required onChange</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading || newValue === '') {
		setDisplay(newValue);
		return;
	}
	
	//Type appropriate comment here, and begin script below
	setDisplay(newValue);
}

function setDisplay(newValue){
	g_form.setDisplay('mobile_international_calls', false);
	g_form.setDisplay('mobile_data_roaming', false);
	
	if (newValue == 'true'){
		g_form.setDisplay('mobile_international_calls',true);
		g_form.setDisplay('mobile_data_roaming',true);
	}
}]]></script>
