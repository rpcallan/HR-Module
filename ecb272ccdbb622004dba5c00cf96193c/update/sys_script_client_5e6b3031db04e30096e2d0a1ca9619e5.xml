        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading || newValue === '') {
		setDisplay(newValue);
		return;
	}
	
	//Type appropriate comment here, and begin script below
	setDisplay(newValue);
}

function setDisplay(newValue){
	g_form.setMandatory('monitor_qty', false);
	g_form.setDisplay('monitor_qty', false);
	
	if (newValue == 'true'){
		g_form.setDisplay('monitor_qty', true);
		g_form.setMandatory('monitor_qty', true);
	}
}]]></script>
