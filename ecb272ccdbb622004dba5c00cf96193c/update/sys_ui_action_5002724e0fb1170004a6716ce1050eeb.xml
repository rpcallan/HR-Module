        <name>Cancel Leaver Request</name>
        <onclick/>
        <order>600</order>
        <script><![CDATA[current.state = 3;
current.active = false;

//cancel related request item
var req_item = new GlideRecord('sc_req_item');
req_item.get(current.request_item);
req_item.state = '7';
req_item.stage = 'Request Cancelled';
req_item.worknotes = 'Cancelled from Leaver Request ' + current.number;
req_item.active = false;
req_item.update();

current.update();
action.setRedirectURL(current);]]></script>
